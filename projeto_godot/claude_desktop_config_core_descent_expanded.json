{
  "mcpServers": {
    "godot-mcp-core-descent": {
      "command": "node",
      "args": ["/workspace/godot-mcp/server/dist/index.js"],
      "env": {
        "MCP_TRANSPORT": "stdio",
        "GODOT_PROJECT_PATH": "/workspace/projeto_godot"
      },
      "capabilities": {
        "tools": {
          "listChanged": false,
          "subscribe": false
        },
        "resources": {
          "subscribe": false,
          "listChanged": false
        },
        "prompts": {
          "listChanged": false
        },
        "logging": {}
      }
    }
  }
}